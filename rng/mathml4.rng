<grammar ns="http://www.w3.org/1998/Math/MathML" xmlns="http://relaxng.org/ns/structure/1.0">
  <!-- Presentation MathML -->
  <include href="mathml4-presentation.rng">
    <define name="anyElement">
      <element>
        <anyName>
          <except>
            <nsName/>
          </except>
        </anyName>
        <zeroOrMore>
          <choice>
            <attribute>
              <anyName/>
            </attribute>
            <text/>
            <ref name="anyElement"/>
          </choice>
        </zeroOrMore>
      </element>
    </define>
  </include>
  <!-- Content  MathML -->
  <include href="mathml4-content.rng"/>
</grammar>
